!! URxvt Appearance
URxvt.font: 			xft:UbuntuMono:size=14,xft:3270Medium\ Nerd\ Font\ Mono:size=14
URxvt.letterSpace: 		-1
URxvt.internalBorder: 		15
URxvt.cursorBlink: 		true
URxvt.cursorUnderline: 		false
URxvt.saveline: 		2048
URxvt.scrollBar: 		false
URxvt.scrollBar_right: 		false
URxvt.urgentOnBell: 		true
URxvt.depth: 			32

!! Black
#define _color0			#0d0d0d
#define _color8			#1a1a1a

!! Red
#define _color1			#CC0000
#define _color9			#EF2929

!! Green
#define _color2			#EF2929
#define _color10		#EF2929

!! Yellow
#define _color3			#ff3333
#define _color11		#ffb3b3

!! Blue
#define _color4			#bb3333
#define _color12		#ff4444

!! Magenta
#define _color5			#712525
#define _color13		#BE2929

!! Cyan
#define _color6			#EF2929
#define _color14		#FF6363

!! White
#define _color7			#D3D7CF
#define _color15		#EEEEEC

#define _background		#000000
#define _foreground		#FFFFFF

*color0: 			_color0
*color1: 			_color1
*color2: 			_color2
*color3: 			_color3
*color4: 			_color4
*color5: 			_color5
*color6: 			_color6
*color7: 			_color7
*color8: 			_color8
*color9: 			_color9
*color10: 			_color10
*color11: 			_color11
*color12: 			_color12
*color13: 			_color13
*color14: 			_color14
*color15: 			_color15

URxvt.background: 		_background
URxvt.foreground: 		_foreground

URxvt.perl-ext-common: 		default,matcher,resize-font,clipboard,url-select,keyboard-select,vtwheel,autocomplete-ALL-the-things
URxvt.keysym.M-u:		perl:url-select:select_next
URxvt.url-select.underline:	true
URxvt.url-select.launcher:	/usr/bin/xdg-open
URxvt.keysym.M-Escape:		perl:keyboard-select:activate
URxvt.keysym.M-s:		perl:keyboard-select:search
URxvt.matcher.button:		1	
URxvt.resize-font.smaller:	C-j
URxvt.resize-font.bigger:	C-k
URxvt.keysym.M-c: 		perl:clipboard:copy
URxvt.keysym.M-v: 		perl:clipboard:paste
URxvt.keysym.M-slash:		perl:aAtt:complete

Xcursor.theme: 			Bibata_Classic
